account leo.hubert@mailo.cat
host mail.mailo.com
port 465
from leo.hubert@mailo.cat
user leo.hubert@mailo.cat
# passwordeval "pass leo.hubert@mailo.cat"
auth on
tls on
tls_trust_file	/etc/ssl/cert.pem
logfile /Users/leo/.config/msmtp/msmtp.log
tls_starttls off

account leo.hubert@univ-amu.fr
host smtp.univ-amu.fr
port 587
from leo.hubert@univ-amu.fr
user leo.hubert@univ-amu.fr
# passwordeval "pass leo.hubert@univ-amu.fr"
auth on
tls on
tls_trust_file	/etc/ssl/cert.pem
logfile /Users/leo/.config/msmtp/msmtp.log
tls_starttls on
